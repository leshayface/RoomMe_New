<p id="notice"><%= notice %></p>

<h1>About Rooms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_rooms.each do |about_room| %>
      <tr>
        <td><%= link_to 'Show', about_room %></td>
        <td><%= link_to 'Edit', edit_about_room_path(about_room) %></td>
        <td><%= link_to 'Destroy', about_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About Room', new_about_room_path %>
