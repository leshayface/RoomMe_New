<p id="notice"><%= notice %></p>

<h1>Housemates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @housemates.each do |housemate| %>
      <tr>
        <td><%= link_to 'Show', housemate %></td>
        <td><%= link_to 'Edit', edit_housemate_path(housemate) %></td>
        <td><%= link_to 'Destroy', housemate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Housemate', new_housemate_path %>
