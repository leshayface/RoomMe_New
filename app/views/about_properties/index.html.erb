<p id="notice"><%= notice %></p>

<h1>About Properties</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_properties.each do |about_property| %>
      <tr>
        <td><%= link_to 'Show', about_property %></td>
        <td><%= link_to 'Edit', edit_about_property_path(about_property) %></td>
        <td><%= link_to 'Destroy', about_property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About Property', new_about_property_path %>
